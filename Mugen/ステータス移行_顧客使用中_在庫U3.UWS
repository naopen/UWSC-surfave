	WHILE (CHKIMG("data\lookup_selection.BMP")=FALSE)
	SLEEP(0.01)
		IF CHKIMG("data\lookup_nodata.BMP")=TRUE OR CHKIMG("data\lookup_success.BMP")=TRUE THEN BREAK
