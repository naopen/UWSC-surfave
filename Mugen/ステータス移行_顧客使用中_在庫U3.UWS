		SLEEP(0.1)
		WHILE (CHKIMG("data\lookup_nodata.BMP")=FALSE)
		SLEEP(0.01)
			IF CHKIMG("data\lookup_success.BMP")=TRUE OR CHKIMG("data\lookup_selection.BMP")=TRUE THEN BREAK
